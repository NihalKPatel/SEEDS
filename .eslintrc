{
    "extends": [
        "react-app",
        "airbnb",
        "plugin:jsx-a11y/recommended",
        "prettier"
    ],
    "plugins": [
        "jsx-a11y",
        "prettier"
    ],
    "rules": {
        "semi": 0,
        "import/extensions": 0,
        "no-underscore-dangle": 0,
        "react/jsx-filename-extension": [
            1, 
            {"extensions": 
                [
                    ".js",
                     ".jsx"
                ]
            }
        ],
        "prettier/prettier": [
            "error",
            {
                "semi": false,
                "endOfLine": "auto"
            }
        ]
    }
}